{
  "name": "Windows 11 Pro",
  "service": "windows",
  "containerEnv": {
    "VERSION": "11"
  },
          "features": {
            "ghcr.io/devcontainers/features/sshd:1": {
              "version": "latest"
            }
          },
  "forwardPorts": [8006],
  "portsAttributes": {
    "8006": {
      "label": "Web",
      "onAutoForward": "notify"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "ignore"
  },
  "dockerComposeFile": "codespaces.yml",
  "initializeCommand": "docker system prune --all --force"
}
